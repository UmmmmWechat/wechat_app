<template name="OrderCardGuide$6b186685">
  <view class="_div data-v-a0d41a3a d-card" style=" {{('background-color:' + color + ';')}}">
    <view class="_div data-v-a0d41a3a error-wrapper" wx:if="{{errorOccur}}"> 粗错啦 QWQ </view>
    <view wx:else class="_div data-v-a0d41a3a">
      <view bindtap="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" class="_section data-v-a0d41a3a">
        <view class="_div data-v-a0d41a3a head"><label class="_span data-v-a0d41a3a">{{order.state}}</label></view>
        <view id="body" class="_div data-v-a0d41a3a">
          <view class="_div data-v-a0d41a3a"><label class="_span data-v-a0d41a3a title-span">景点：</label> <label class="_span data-v-a0d41a3a">{{ spotName }}</label></view>
          <view class="_div data-v-a0d41a3a"><label class="_span data-v-a0d41a3a title-span">游客编号：</label> <label class="_span data-v-a0d41a3a">{{ order.touristId }}</label></view>
          <view class="_div data-v-a0d41a3a"><label class="_span data-v-a0d41a3a title-span">邀请日期：</label>{{ computedCreatedDate }}</view>
          <view class="_div data-v-a0d41a3a"><label class="_span data-v-a0d41a3a title-span">旅游日期：</label>{{ computedTravelDate }}</view>
        </view>
      </view>
      <view id="foot" wx:if="{{order.state === waiting}}" class="_div data-v-a0d41a3a"><button class="_button data-v-a0d41a3a op-btn" size="mini" bindtap="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}"> 婉拒 </button> <button class="_button data-v-a0d41a3a d-a op-btn" size="mini" bindtap="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}"> 同意 </button></view>
    </view>
  </view>
</template>